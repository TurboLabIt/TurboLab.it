        <link rel="preload" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
            as="style" onload="this.onload=null;this.rel='stylesheet'">

        <link rel="preload" href="{{ asset('build/app.css') }}"
            as="style" onload="this.onload=null;this.rel='stylesheet'">

    {% endblock %}

    {% block preloaderStyle %}{% include 'parts/preloader.html.twig' %}{% endblock %}

</head>

<body class="home-1-bg">

    {% block preloaderHtml %}<div class="preloader"><div><div class="nb-spinner"></div></div></div>{% endblock %}

    <div id="tli-userbar-load-target" data-loading-url="{{ path('app_cache-hole-punching_userbar') }}">
        <div class="container" style="padding: 10px">
            <div class="col-12">
                <i class="fa-solid fa-spinner fa-spin-pulse" style="color: #000000;"></i>
                Sei gi√† dei nostri? Login in corso...
            </div>
        </div>
    </div>

    <header class="header-area">

        <div class="header-centerbar">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-4">
                        <div class="logo">
                            <a href="{{ path('app_home') }}">
                                <img src="{{ asset('images/logo/turbolab.it.png') }}"
                                     width="416" height="71" alt="Il logo di TurboLab.it">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="header-menubar">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-12">
                        <div class="header-menu">
                            <div class="stellarnav">
                                <ul>
                                    <li>
                                        <a class="{{ activeMenu == 'home' ? 'active' : '' }}" href="{{ path('app_home') }}">
                                            <i class="fa-solid fa-house"></i> Home
                                        </a>
                                    </li>
                                    <li class="has-sub">
                                        <a href="{{ path('app_home') }}forum">
                                            <i class="fa-solid fa-users"></i> Forum <i class="fa-solid fa-angle-down"></i>
                                        </a>
                                        <ul>
                                            <li><a href="{{ path('app_home') }}forum/search.php?search_id=egosearch">I tuoi messaggi</a></li>
                                            <li><a href="{{ path('app_home') }}forum/search.php?search_id=unreadposts">Messaggi non letti</a></li>
                                            <li><a href="{{ path('app_home') }}forum/ucp.php?i=pm&amp;folder=inbox">Messaggi privati</a></li>
                                        </ul>
                                    </li>

                                    {% for Link in FrontendHelper.NavTopCategories %}
                                        <li>
                                            <a class="{{ activeMenu == Link.activeMenu ? 'active' : '' }}" href="{{ Link.url }}">
                                                {% for fa in Link.fa %}
                                                    <i class="{{ fa }}"></i>
                                                {% endfor %} &nbsp;{{ Link.name }}
                                            </a>
                                        </li>
                                    {% endfor %}

                                    <li class="has-sub">
                                        <a class="{{ activeMenu == 'guide' ? 'active' : '' }} disabled-link" href="#">
                                            <i class="fa-solid fa-book"></i> Guide <i class="fa-solid fa-angle-down"></i>
                                        </a>
                                        <ul>
                                            {% for Link in FrontendHelper.NavOtherCategories %}
                                                <li>
                                                    <a href="{{ Link.url }}">
                                                        <i class="{{ Link.fa|first }}"></i> &nbsp;{{ Link.name }}
                                                    </a>
                                                </li>
                                            {% endfor %}
                                        </ul>
                                    </li>

                                    <li class="has-sub">
                                        <a class="{{ activeMenu == 'cerca' ? 'active' : '' }} disabled-link" href="#">
                                            <i class="fa-solid fa-magnifying-glass" style="color: #e66100;"></i>
                                            Cerca <i class="fa-solid fa-angle-down"></i>
                                        </a>
                                        <ul id="tli-nav-search">
                                            <li>
                                                <form action="{{ path('app_search') }}" class="submit-to-path" method="GET">
                                                    <div class="form-group">
                                                        <input type="text" class="form-control submit-to-path" name="query" required
                                                               placeholder="windows su usb" value="{{ (termToSearch ?? null)|raw }}"
                                                               maxlength="255" style="margin-bottom: 7px;">
                                                        <button type="submit" class="btn btn-primary">
                                                            <i class="fa-solid fa-magnifying-glass"></i> Cerca su TurboLab.it
                                                        </button>
                                                    </div>
                                                </form>
                                            </li>
                                        </ul>
                                    </li>

                                    <li>
                                        <a href="#contact" class="prevent-change-url">
                                            <i class="fa-solid fa-envelope" style="color: #e01b24;"></i> Contatti
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </header>

    {% block bodyMainContent %}{% endblock %}

    {% include '@TurboLabItPaginator/pagination-bootstrap.html.twig' %}

    {% include 'user/login-modal.html.twig' %}

